# to writeing information on subscreen

def drawing_information(int):
    global match_list
    font = cv2.FONT_HERSHEY_SIMPLEX 
    for i in match_list:
        if i[0] == int:
            cv2.putText(screen_gray, screen_text, (i[1][0], i[1][1]), font, 1, (0, 0, 0), 2)
        if i[0] == int:
            cv2.putText
    return

